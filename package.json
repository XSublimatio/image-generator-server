{
  "name": "image-generator-server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "build": "prisma generate && tsc",
    "generate-prisma": "prisma generate",
    "docker:run": "APP_ENV=development prisma generate && prisma migrate deploy && yarn build && yarn start",
    "build:docker:prod": "docker build . -t my-fastify-app --build-arg APP_ENV=production",
    "start": "node build",
    "dev": "nodemon | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "NODE_ENV=test tap",
    "test:watch": "NODE_ENV=test tap -w",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XSublimatio/image-generator-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/XSublimatio/image-generator-server/issues"
  },
  "homepage": "https://github.com/XSublimatio/image-generator-server#readme",
  "dependencies": {
    "@faction-nfts/xsublimatio-smart-contracts": "^0.7.9",
    "@prisma/client": "^3.14.0",
    "@sinclair/typebox": "^0.23.4",
    "cloudinary": "^1.29.1",
    "env-schema": "^3.5.2",
    "ethers": "^5.6.0",
    "fastify": "^3.27.2",
    "fastify-cors": "^6.0.3",
    "fastify-now": "^2.5.2",
    "git": "^0.1.5",
    "queue-ts": "^0.1.0",
    "systeminformation": "^5.11.5",
    "tiny-typed-emitter": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/tap": "^15.0.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "prisma": "^3.14.0",
    "tap": "^15.1.6",
    "typescript": "~4.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.js": "eslint --cache --fix"
  }
}
